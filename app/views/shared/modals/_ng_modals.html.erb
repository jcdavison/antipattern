<% if current_user %>
  <script type="text/ng-template" id="submitOffer.html">
    <%= render 'shared/modals/submit_offer' %>
  </script>
  <script type="text/ng-template" id="requestCodeReview.html">
    <%= render 'shared/modals/request_code_review' %>
  </script>
  <script type="text/ng-template" id="editCodeReview.html">
    <%= render 'shared/modals/edit_review_request' %>
  </script>
<% else %>
  <script type="text/ng-template" id="pleaseLogin.html">
    <%= render 'shared/modals/please_login' %>
  </script>
<% end %>
